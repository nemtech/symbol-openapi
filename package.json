{
  "name": "nem2-openapi",
  "version": "0.8.1",
  "description": "OpenAPI Specification of catapult-rest",
  "main": "index.js",
  "scripts": {
    "test": "swagger-cli validate _build/openapi3.yml",
    "build": "swagger-cli bundle ./spec/openapi.yml --outfile _build/openapi3.yml --type yaml",
    "postman": "openapi2postmanv2 -s _build/openapi3.yml -o _build/postman.json -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nemtech/nem2-openapi.git"
  },
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/nemtech/nem2-openapi/issues"
  },
  "homepage": "https://github.com/nemtech/nem2-openapi#readme",
  "dependencies": {
    "openapi-to-postmanv2": "^0.1.0"
  }
}
